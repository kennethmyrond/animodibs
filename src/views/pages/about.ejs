            <li>connect-mongo</li>
            <li>cookie-session</li>
            <li>dotenv</li>
            <li>ejs</li>
            <li>express</li>
            <li>express-handlebars</li>
            <li>express-session</li>
            <li>moment</li>
            <li>mongodb</li>
            <li>mongoose</li>
            <li>morgan</li>
            <li>nodemon</li> 
            <li>passport</li>
            <li>passport-google-oauth</li>       
        </ul>
        <h3><b>Third-Party Libraries</b></h3>
        <ul>
            <li>Bootstrap</li>
            <li>daterangepicker</li>
            <li>Moment</li>
            <li>Popper</li>      
        </ul>
